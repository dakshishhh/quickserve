    function gotoCategory(id) {
      var element = document.getElementById(id);
      element.scrollIntoView({
        behavior: "smooth",
        block: "center",
      });
      document.body.scrollTop = element.offsetTop - 200;
      var activeElement = document.getElementsByClassName('active');
      if (activeElement.length) {
        activeElement[0].classList.remove('active')
      }
      var tabElement = document.getElementById('new-' + id);
      tabElement.classList.add('active');

    }
    function formShow() {
      document.getElementById('section_demo_form').style.display = 'block'
    }

    function formClose() {
      document.getElementById('section_demo_form').style.display = 'none'
    }


