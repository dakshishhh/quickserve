  <script type="text/javascript">
    function numbersonly(myfield, e, dec) {
      var key;
      var keychar;
      if (window.event)
        key = window.event.keyCode;
      else if (e)
        key = e.which;
      else
        return true;
      keychar = String.fromCharCode(key);
      // control keys
      if ((key == null) || (key == 0) || (key == 8) ||
        (key == 9) || (key == 13) || (key == 27))
        return true;
      // numbers
      else if ((("0123456789").indexOf(keychar) > -1))
        return true;
      // decimal point jump
      else if (dec && (keychar == ".")) {
        myfield.form.elements[dec].focus();
        return false;
      }
      else
        return false;
    }
  </script>
  <!-- Validate demo form  -->
     <script>
	

		function stateHandle() 
		{
		   if($("#name").val()=="")
			{
				$("#btnSubmit").addClass("disabled");
				 
			} 
			else 
			{
				$("#btnSubmit").removeClass("disabled");
			}
		}
        $("#request_demo_form").validate({
            rules: {
                name: "required",
                phone: "required",
				email: "required",
                
            },
            messages: {
                name: "Enter Name",
                phone: "Enter Phone",
				email: "Enter Email Id",
                
            },
            submitHandler: function(form) {
                //return false;
                event.preventDefault();
                $this = $(form).find('#sendFeedbackInfo_2');
                $this.prop("disabled", true); // Disable submit button until AJAX call is complete to prevent duplicate messages
                var data_json = {
                    name: $("#request_demo_form #name").val(),
                    phone: $("#request_demo_form #phone").val(),
                    email: $("#request_demo_form #email").val(),
                    businessCategory: $('input[name="business_category"]:checked').val(),
                    source: window.location.href,
                    // source: "landing-https://dotpe.in/landing-ecomm.html?utm_source=google&utm_medium=cpc&utm_campaign=Ecomm_Search_Generic_em&utm_term=free%20website%20builder",
                    tncCheck: true
                };
                $.ajax({
                    type: "post",
                    url: "curl.php",
                    data:  data_json,
                    cache: false,
                    success: function(res) {

                        // Success message
                        console.log(res);

						$('#requestDemoModal').modal('hide');
						
                         $('#thankyouModal').modal('show');
                            // console.log(res.status);
                            //Swal.fire('Thank you for Contacting!', '', 'success');
                        
                        //$(form).trigger("reset");
								setTimeout(function() {
                             $('#thankyouModal').modal('hide');
$(form).trigger("reset");							 // Re-enable submit button when AJAX call is complete
                        }, 5000);						
                    },
                    error: function(err) {
                        // alert(err);
                        console.log(err);
						Swal.fire('Try again!', '', 'error');
                        
                        //clear all fields
                         $(form).trigger("reset");
                    },
                    complete: function() {
                        setTimeout(function() {
                            $this.prop("disabled", false); // Re-enable submit button when AJAX call is complete
                        }, 1000);
                    }
                });
            },
            invalidHandler: function(event, validator) {
                // 'this' refers to the form
                var errors = validator.numberOfInvalids();
                if (errors) {
                    var message = errors == 1 ?
                        'You missed 1 field. It has been highlighted' :
                        'You missed ' + errors + ' fields. They have been highlighted';
                    $("div.error span").html(message);
                    $("div.error").show();
                } else {
                    $("div.error").hide();
                }
            }
        });
		
		function callpopup()
		{
				var phoneno=$("#txtPhoneno").val();
				localStorage.setItem('Phone', phoneno);
				var PhoneNo = localStorage.getItem('Phone');
				if (PhoneNo == null || PhoneNo == "null") {
		
				}
				else
				{
				var phoneno=parseInt(PhoneNo);
				
				$('input[name="phone"]').val(phoneno);
				
				
				}
				$('#requestDemoModal').modal('show');
				$("#txtPhoneno").val('');
				return false;
		}
		
     </script> 
</body>

</html>